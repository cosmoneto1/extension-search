{
  "name": "Search bing",
  "version": "0.0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.ts"
  },
  "omnibox": {
    "keyword": "bing"
  },
  "action": {
    "default_icon": {
      "16": "icon.png",
      "32": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": ["activeTab"]
}
